{
    "name": "name/upgrade-pack",
    "description": "Symfony Upgrade Pack - A tool to help upgrade Symfony applications",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "symfony/routing": "7.2.*",
        "symfony/http-foundation": "7.2.*",
        "symfony/http-kernel": "7.2.*",
        "symfony/yaml": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/framework-bundle": "7.2.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/phpstan": "^2.1.11",
        "phpunit/phpunit": "^12.0.10",
        "rector/rector": "^2.0.11",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    }
}
